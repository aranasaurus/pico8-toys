pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
heart_cloud={}
spawn_rate=33 
spawner=3

function make_heart(x,y,init_size,c)
 local h={}
 h.x=x
 h.y=y
 h.c=c
 h.w=init_size
 h.w_max=init_size+rnd(2)+1
 h.t=0
 h.t_max=30+rnd(16)
 h.dx=rnd(1)-0.4
 h.dy=rnd(0.5)
 h.ddy=0.04
 h.is_cloud=rnd(1)<0.2
 h.is_rainbow=rnd(1)<0.3
 add(heart_cloud,h)
 return h
end

function move_heart(h)
 if h.t>h.t_max then
  del(heart_cloud,h)
 end
 if h.t>h.t_max-15 then
  h.w=min(1+h.w,h.w_max)
 end
 h.x+=h.dx
 h.y+=h.dy
 h.dy-=h.ddy
 h.t+=1
end

function draw_heart(h)
 if h.is_rainbow then
  spr(0,h.x,h.y)
 elseif h.is_cloud then
  circfill(h.x,h.y,h.w,h.c)
 else
  spr(1,h.x,h.y)
 end
end

function _init()
 cursorx=50
 cursory=50
 color=7
end

function _update()
 foreach(heart_cloud,move_heart)
 if (btn(0)) then cursorx-=2 end
 if (btn(1)) then cursorx+=2 end
 if (btn(2)) then cursory-=2 end
 if (btn(3)) then cursory+=2 end
 if btn(5) then 
  spawn_rate=spawn_rate+1 
  if spawn_rate==100 then
   spawn_rate=10
  end
 end
 if btnp(4) then
  if spawner==3 then
   spawner=2
  else
   spawner=3
  end
 end
 if (rnd(100)<spawn_rate)then make_heart(cursorx,cursory,rnd(3),color) end
end

function _draw()
 cls()
 foreach(heart_cloud,draw_heart)
 spr(spawner,cursorx+3,cursory-3)
 print(spawn_rate,0,0,6)
end
